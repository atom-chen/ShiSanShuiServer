{
    "name": "dealer root",
    "control_type": "selector",
    "child": [
        {
            "name": "case player enter",
            "case": "enter",
            "control_type": "sequence",
            "child": [
                {
                    "name": "dealer do player enter",
                    "control_type": "step",
                    "do_script": "flow_dealer.do_player_enter"
                }
            ]
        },
        {
            "name": "dealer tick",
            "case": "timeout",
            "control_type": "sequence",
            "child": [
                {
                    "name": "dealer_stage_choose",
                    "control_type": "step",
                    "do_script": "flow_dealer.check_is_all_player_free"
                },
                {
                    "name": "selector dealer_stage_choose",
                    "control_type": "selector",
                    "select_type": "step_return",
                    "step_name": "flow_dealer.dealer_stage_choose",
                    "child": [
                        {
                            "name": "case prepare",
                            "case": "prepare",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "dealer check_to_start_game",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.check_to_start_game"
                                },
                                {
                                    "name": "dealer start_game",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.start_game"
                                }
                            ]
                        },
                        {
                            "name": "case mult",
                            "case": "mult",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "selector check_is_mult_end",
                                    "control_type": "selector",
                                    "select_type": "step_return",
                                    "step_name": "flow_dealer.check_is_mult_end",
                                    "child": [
                                        {
                                            "case": "no",
                                            "name": "deal call mult",
                                            "control_type": "step",
                                            "do_script": "flow_dealer.call_player_mult"
                                        },
                                        {
                                            "case": "yes",
                                            "control_type": "step",
                                            "do_script": "flow_dealer.do_mult_end"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "name": "case deal",
                            "case": "deal",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "deal_do_deal",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.do_deal"
                                }
                            ]
                        },
                        {
                            "name": "case choose",
                            "case": "choose",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "selector check_is_choose_cardtype_end",
                                    "control_type": "selector",
                                    "select_type": "step_return",
                                    "step_name": "flow_dealer.check_is_choose_cardtype_end",
                                    "child": [
                                        {
                                            "case": "no",
                                            "name": "deal call choose",
                                            "control_type": "step",
                                            "do_script": "flow_dealer.call_player_choose_cardtype"
                                        },
                                        {
                                            "case": "yes",
                                            "control_type": "step",
                                            "do_script": "flow_dealer.do_choose_cardtype_end"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "name": "case compare",
                            "case": "compare",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "selector check_is_compare_wait",
                                    "control_type": "selector",
                                    "select_type": "step_return",
                                    "step_name": "flow_dealer.check_is_compare_wait",
                                    "child": [
                                        {
                                            "case": "no",
                                            "control_type": "sequence",
                                            "child": [
                                                {
                                                    "name": "notify_compare_start",
                                                    "control_type": "step",
                                                    "do_script": "flow_dealer.notify_compare_start"
                                                },
                                                {
                                                    "name": "do_compare_result",
                                                    "control_type": "step",
                                                    "do_script": "flow_dealer.do_compare_result"
                                                },
                                                {
                                                    "name": "notify_compare_result",
                                                    "control_type": "step",
                                                    "do_script": "flow_dealer.notify_compare_result"
                                                }
                                            ]
                                        },
                                        {
                                            "case": "yes",
                                            "control_type": "step",
                                            "do_script": "flow_dealer.do_compare_wait"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "name": "case reward",
                            "case": "reward",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "do_thisgame_reward",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.do_thisgame_reward"
                                },
                                {
                                    "name": "notify_game_reward",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.notify_game_reward"
                                }
                            ]
                        },
                        {
                            "name": "case end",
                            "case": "gameend",
                            "control_type": "sequence",
                            "child": [
                                {
                                    "name": "check_to_game_end",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.check_to_game_end"
                                },
                                {
                                    "name": "do_thisgame_end",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.do_thisgame_end"
                                },
                                {
                                    "name": "prepare_next",
                                    "control_type": "step",
                                    "do_script": "flow_dealer.prepare_next"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}